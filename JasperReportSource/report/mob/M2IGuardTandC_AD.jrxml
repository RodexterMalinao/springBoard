						<font fontName="PCCW-zh-Ming" size="8"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["<br>This Service Contract will automatically terminate:"]]></textFieldExpression>
				</textField>
			</frame>
			<frame>
				<reportElement positionType="Float" x="288" y="130" width="250" height="10"/>
