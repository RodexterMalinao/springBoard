			<textField isStretchWithOverflow="true">
				<reportElement x="18" y="0" width="250" height="10"/>
				<textElement verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["<b>1. <u>DEFINITIONS</u></b>"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="10" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Accidental</b>\" means sudden, unforeseen and unexpected, and happening by chance."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="20" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Customer Information Form</b>\" means Your completed and accepted information form for the i-GUARD <b>Phone&Tablet<i>Protector</i> LDS Plus Handset Reimbursement Plan</b> which together with these terms and conditions forms the Service Contract."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="30" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>CSL Mobile</b>\" means CSL Mobile Limited and/or any of its holding companies,subsidiaries or subsidiaries of any such holding companies which sell(s) Handset or operate(s) mobile telecommunications networks in Hong Kong."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="40" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Damaged Beyond Repair</b>\" means a mobile phone handset or tablet whcih has been comfirmed by the mobile phone manufacturer or its appointed service center to be damaged beyond repair or the cost of repair is greater than the handset or tablet value."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="50" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Handset</b>\" means a mobile phone handset or tablet (including but not limited to iPad), able to be connected to one or more networks operated by CSL Mobile."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="60" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Loss</b>\" means a permanent and involuntary failure to keep possession of property. For the avoidance of doubt, Loss does not include instances when You sell or give away the Handset (as defined in paragraph 2)."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="70" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>LDS</b>\" means \"<b>Lost/Damaged Beyond Repair/Stolen</b>\"."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="80" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Reimbursement Amount</b>\" means the percentage of the original Handset purchase price to be reimbursed under this Service Contract being:"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="90" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["If the reimbursement is approved:"]]></textFieldExpression>
			</textField>
			<frame>
				<reportElement positionType="Float" x="18" y="100" width="250" height="10"/>
				<textField isStretchWithOverflow="true">
					<reportElement positionType="Float" x="0" y="0" width="20" height="10"/>
					<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
						<font fontName="PCCW-zh-Ming" size="9"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["a."]]></textFieldExpression>
				</textField>
				<textField isStretchWithOverflow="true">
					<reportElement positionType="Float" x="20" y="0" width="230" height="10"/>
					<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
						<font fontName="PCCW-zh-Ming" size="9"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["60% of the original Handset cost up to a maximum of HK\$5,000 in year 1 and 40% of the original Handset cost up to a maximum of HK\$3,500 in year 2 of this Service Contract;and"]]></textFieldExpression>
				</textField>
			</frame>
			<frame>
				<reportElement positionType="Float" x="18" y="110" width="250" height="10"/>
				<textField isStretchWithOverflow="true">
					<reportElement positionType="Float" x="20" y="0" width="230" height="10"/>
					<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
						<font fontName="PCCW-zh-Ming" size="9"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["if original Handset model is no longer available, You shall purchase a Handset from same manufacturer at 1010 or csl shop; or if no other model from the same manufacturer is available at all 1010 or csl shops, You shall purchase a Handset from another manufacturer. You are not allowed however to replace a Handset of higher specification, for example, you are not allowed to replace a second generation (2G) or third generation (3G) Handset with a Handset using fourth generation standards (4G or equivalent)"]]></textFieldExpression>
				</textField>
				<textField isStretchWithOverflow="true">
					<reportElement positionType="Float" x="0" y="0" width="20" height="10"/>
					<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
						<font fontName="PCCW-zh-Ming" size="9"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA["b."]]></textFieldExpression>
				</textField>
			</frame>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="120" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Service Contract</b>\" has the meaning described in paragraph 2."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="130" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>service fee</b>\" means such fees, whether in the form of monthly subscription or in one or more installment(s), payable by You under this Service Contract."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="140" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>Stolen</b>\" means the taking and removing of property by another person without consent and with the intent to permanently deprive the true owner of possession, including by force, intimidation or deception."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="150" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>We</b>\", \"<b>Us</b>\" and \"<b>Our</b>\" refers to i-Guard Direct Limited."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="18" y="160" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["\"<b>You</b>\" and \"<b>Your</b>\" refers to purchaser of this i-GUARD <b>Phone & Tablet<i>Protector</i> LDS Plus Handset Reimbursement Plan</b>."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
