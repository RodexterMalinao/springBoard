					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["<b>3. <u>RECORD KEEPING</u></b>"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="288" y="50" width="250" height="10"/>
				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Although We maintain a record of Your Service Contract, We recommend that You place these documents in a safe place. This Service Contract is personal to You and may not be assigned without our agreement."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="288" y="70" width="250" height="10"/>
