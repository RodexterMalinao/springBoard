				<textElement textAlignment="Justified" verticalAlignment="Top" markup="none">
					<font fontName="PCCW-Arial" size="9" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["If you do not pay any amount due under this Agreement on time, we may:"]]></textFieldExpression>
			</textField>
		</band>
		<band height="20" splitType="Stretch">
