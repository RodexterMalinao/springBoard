				<textElement textAlignment="Justified" verticalAlignment="Middle" markup="html">
					<font fontName="PCCW-zh-Ming" size="9" isBold="false"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["See your PCCW-HKT mobile service Application to identify which of the core services below you have selected to."]]></textFieldExpression>
			</textField>
		</band>
		<band height="10" splitType="Stretch">
