				<reportElement x="25" y="0" width="510" height="10"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["<b><u>SIM No.:</u></b> This is the number specified on the back side of the SIM Card provided to you."]]></textFieldExpression>
			</textField>
		</band>
		<band height="10" splitType="Prevent">
