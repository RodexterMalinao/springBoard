					<font fontName="PCCW-zh-Ming" size="9" isBold="true"/>
				</textElement>
				<text><![CDATA[SECTION B   CONTRACT TERM]]></text>
			</staticText>
		</band>
		<band height="22" splitType="Prevent">
			<rectangle>
				<reportElement x="16" y="3" width="4" height="4" backcolor="#000000"/>
