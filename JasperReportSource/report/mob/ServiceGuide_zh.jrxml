				<reportElement x="25" y="0" width="510" height="10"/>
				<textElement textAlignment="Justified" markup="html">
					<font fontName="PCCW-zh-Ming" size="9"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["<b><u>攜機上台：</u></b> 只訂購服務計劃。"]]></textFieldExpression>
			</textField>
		</band>
		<band height="10" splitType="Prevent">
